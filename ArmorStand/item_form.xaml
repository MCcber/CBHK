<Window x:Name="ItemForm" x:Class="ArmorStand.Item_form"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" Height="301" Width="400" WindowStartupLocation="CenterScreen" Closing="ItemForm_Closing" Background="#FF141414">
    <Grid>
        <TextBox x:Name="ItemInfoBox" TextWrapping="Wrap" Foreground="#FFA8A8A8">
            <TextBox.Background>
                <SolidColorBrush Color="White" Opacity="0"/>
            </TextBox.Background>
        </TextBox>

    </Grid>
</Window>
