                        <Label Content="X:" Grid.Row="5" Grid.Column="0" Margin="0,20,98,45" Foreground="#FFA8A8A8"></Label>
                        <Slider x:Name="rleg_x" Grid.Row="5" Grid.Column="0" Margin="19,20,0,45" Value="0" Minimum="-180" Maximum="180" ValueChanged="Rleg_x_ValueChanged"></Slider>
                        <TextBox x:Name="rleg_x_value" Grid.Row="5" Grid.Column="0" Margin="0,44,0,0" MaxLines="1" MinLines="1" Text="0" Foreground="#FFA8A8A8" BorderBrush="{x:Null}">
                            <TextBox.Background>
                                <SolidColorBrush Color="White" Opacity="0"/>
                            </TextBox.Background>
                        </TextBox>

                        <Label Content="Y:" Grid.Row="5" Grid.Column="1" Margin="0,20,98,45" Foreground="#FFA8A8A8"></Label>
                        <Slider x:Name="rleg_y" Grid.Row="5" Grid.Column="1" Margin="19,20,0,45" Value="0" Minimum="-180" Maximum="180" ValueChanged="Rleg_y_ValueChanged"></Slider>
                        <TextBox x:Name="rleg_y_value" Grid.Row="5" Grid.Column="1" Margin="0,44,0,0" MaxLines="1" MinLines="1" Text="0" Foreground="#FFA8A8A8" BorderBrush="{x:Null}">
                            <TextBox.Background>
                                <SolidColorBrush Color="White" Opacity="0"/>
                            </TextBox.Background>
                        </TextBox>

                        <Label Content="Z:" Grid.Row="5" Grid.Column="2" Margin="0,20,98,45" Foreground="#FFA8A8A8"></Label>
                        <Slider x:Name="rleg_z" Grid.Row="5" Grid.Column="2" Margin="19,20,0,45" Value="0" Minimum="-180" Maximum="180" ValueChanged="Rleg_z_ValueChanged"></Slider>
                        <TextBox x:Name="rleg_z_value" Grid.Row="5" Grid.Column="2" Margin="0,44,0,0" MaxLines="1" MinLines="1" Text="0" Foreground="#FFA8A8A8" BorderBrush="{x:Null}">
                            <TextBox.Background>
                                <SolidColorBrush Color="White" Opacity="0.01"/>
                            </TextBox.Background>
                        </TextBox>

                        <Button x:Name="reset_rleg" Content="重置" Grid.Row="5" Grid.Column="3" Click="Reset_rleg_Click" Foreground="#FFA8A8A8" BorderBrush="{x:Null}">
                            <Button.Background>
                                <SolidColorBrush Color="#FFDDDDDD" Opacity="0"/>
                            </Button.Background>
                        </Button>
                    </Grid>
                </Grid>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
